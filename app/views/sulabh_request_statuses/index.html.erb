<p id="notice"><%= notice %></p>

<h1>Listing Sulabh Request Statuses</h1>

<table>
  <thead>
    <tr>
      <th>Request</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_request_statuses.each do |sulabh_request_status| %>
      <tr>
        <td><%= sulabh_request_status.request %></td>
        <td><%= sulabh_request_status.status %></td>
        <td><%= link_to 'Show', sulabh_request_status %></td>
        <td><%= link_to 'Edit', edit_sulabh_request_status_path(sulabh_request_status) %></td>
        <td><%= link_to 'Destroy', sulabh_request_status, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sulabh request status', new_sulabh_request_status_path %>
