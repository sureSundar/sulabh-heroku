<p id="notice"><%= notice %></p>

<h1>Listing Sulabh Policies</h1>

<table>
  <thead>
    <tr>
      <th>Provider</th>
      <th>Policyno</th>
      <th>Scheme</th>
      <th>Premium</th>
      <th>Startdate</th>
      <th>Enddate</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_policies.each do |sulabh_policy| %>
      <tr>
        <td><%= sulabh_policy.provider %></td>
        <td><%= sulabh_policy.policyno %></td>
        <td><%= sulabh_policy.scheme %></td>
        <td><%= sulabh_policy.premium %></td>
        <td><%= sulabh_policy.startdate %></td>
        <td><%= sulabh_policy.enddate %></td>
        <td><%= link_to 'Show', sulabh_policy %></td>
        <td><%= link_to 'Edit', edit_sulabh_policy_path(sulabh_policy) %></td>
        <td><%= link_to 'Destroy', sulabh_policy, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

