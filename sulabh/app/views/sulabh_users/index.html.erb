<p id="notice"><%= notice %></p>

<h1>Listing Sulabh Users</h1>

<table>
  <thead>
    <tr>
      <th>Username</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_users.each do |sulabh_user| %>
      <tr>
        <td><%= sulabh_user.username %></td>
        <td><%= sulabh_user.password %></td>
        <td><%= link_to 'Show', sulabh_user %></td>
        <td><%= link_to 'Edit', edit_sulabh_user_path(sulabh_user) %></td>
        <td><%= link_to 'Destroy', sulabh_user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sulabh user', new_sulabh_user_path %>
