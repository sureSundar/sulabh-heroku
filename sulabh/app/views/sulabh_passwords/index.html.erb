<p id="notice"><%= notice %></p>

<h1>Listing Sulabh Passwords</h1>

<table>
  <thead>
    <tr>
      <th>Password</th>
      <th>Expiry</th>
      <th>Pin</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_passwords.each do |sulabh_password| %>
      <tr>
        <td><%= sulabh_password.password %></td>
        <td><%= sulabh_password.expiry %></td>
        <td><%= sulabh_password.pin %></td>
        <td><%= link_to 'Show', sulabh_password %></td>
        <td><%= link_to 'Edit', edit_sulabh_password_path(sulabh_password) %></td>
        <td><%= link_to 'Destroy', sulabh_password, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Sulabh password', new_sulabh_password_path %>
