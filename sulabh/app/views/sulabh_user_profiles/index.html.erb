<p id="notice"><%= notice %></p>

<h1>Listing Sulabh User Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Aadhar</th>
      <th>Accountno</th>
      <th>Ifsc</th>
      <th>Bank</th>
      <th>Locale</th>
      <th>Occupation</th>
      <th>Monthlyrevenue</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_user_profiles.each do |sulabh_user_profile| %>
      <tr>
        <td><%= sulabh_user_profile.name %></td>
        <td><%= sulabh_user_profile.address %></td>
        <td><%= sulabh_user_profile.email %></td>
        <td><%= sulabh_user_profile.mobile %></td>
        <td><%= sulabh_user_profile.aadhar %></td>
        <td><%= sulabh_user_profile.accountno %></td>
        <td><%= sulabh_user_profile.ifsc %></td>
        <td><%= sulabh_user_profile.bank %></td>
        <td><%= sulabh_user_profile.locale %></td>
        <td><%= sulabh_user_profile.occupation %></td>
        <td><%= sulabh_user_profile.monthlyrevenue %></td>
        <td><%= link_to 'Show', sulabh_user_profile %></td>
        <td><%= link_to 'Edit', edit_sulabh_user_profile_path(sulabh_user_profile) %></td>
        <td><%= link_to 'Destroy', sulabh_user_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

