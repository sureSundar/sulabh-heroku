<p id="notice"><%= notice %></p>

<h1>Listing Sulabh Roles</h1>

<table>
  <thead>
    <tr>
      <th>Role</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sulabh_roles.each do |sulabh_role| %>
      <tr>
        <td><%= sulabh_role.role %></td>
        <td><%= link_to 'Show', sulabh_role %></td>
        <td><%= link_to 'Edit', edit_sulabh_role_path(sulabh_role) %></td>
        <td><%= link_to 'Destroy', sulabh_role, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

